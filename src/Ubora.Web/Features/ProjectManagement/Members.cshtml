@model Ubora.Web.Features.ProjectManagement.MemberListViewModel

<section class="medium-width">
    <h1 class="full-width">Project members</h1>

    <ol>
        @foreach (var member in Model.Members)
        {
            <li>
                @member.FullName @member.UserId
            </li>
        }
    </ol>
</section>
