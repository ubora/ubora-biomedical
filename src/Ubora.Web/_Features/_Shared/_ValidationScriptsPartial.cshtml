<environment names="Development">
  <script
    src="//cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.17.0/jquery.validate.js"
    asp-fallback-src="~/dist/libs/jquery.validate.min.js"
    asp-fallback-test="window.jQuery && window.jQuery.validator">
  </script>
  <script
    src="//cdnjs.cloudflare.com/ajax/libs/jquery-validation-unobtrusive/3.2.6/jquery.validate.unobtrusive.js"
    asp-fallback-src="~/dist/libs/jquery.validate.unobtrusive.js"
    asp-fallback-test="window.jQuery && window.jQuery.validator && window.jQuery.validator.unobtrusive">
  </script>
</environment>

<environment names="Staging,Production">
  <script
    src="//cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.17.0/jquery.validate.min.js"
    asp-fallback-src="~/dist/libs/jquery.validate.min.js"
    asp-fallback-test="window.jQuery && window.jQuery.validator">
  </script>
  <script
    src="//cdnjs.cloudflare.com/ajax/libs/jquery-validation-unobtrusive/3.2.6/jquery.validate.unobtrusive.min.js"
    asp-fallback-src="~/dist/libs/jquery.validate.unobtrusive.js"
    asp-fallback-test="window.jQuery && window.jQuery.validator && window.jQuery.validator.unobtrusive">
  </script>
</environment>
