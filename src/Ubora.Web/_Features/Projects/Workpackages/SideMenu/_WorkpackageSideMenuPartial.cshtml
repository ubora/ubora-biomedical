@model Ubora.Web._Features.Projects.Workpackages.SideMenu.SideMenuViewModel

<aside>
    <ul class="list-sidebar">
        @foreach (var topLevelMenuItem in Model.TopLevelMenuItems)
        {
            @Html.Partial("~/_Features/_Shared/LeftSideMenu/_SideMenuItemPartial.cshtml", topLevelMenuItem)
        }
    </ul>
</aside>

<script>
    $('ul.list-sidebar').last()
        .find('a.checked-status > span').append('<i class="material-icons text-green font-weight-bold font-sm ml-1">check</i>')
        .end()
        .find('a.unlockable-status > span').append('<i class="material-icons text-blue font-sm ml-1">lock</i>');
</script>