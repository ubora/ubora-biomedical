@using Ubora.Web._Features.Projects
@model Ubora.Web._Features.Projects.History.ProjectHistoryViewModel

@{
    ViewData["Title"] = "History";
    ViewData["MenuOption"] = ProjectMenuOption.History;
}

<section class="medium-width">
    <h1 class="full-width">Project history</h1>
    
    <ol>
        @foreach (var eventDescription in Model.Events)
        {
            <li>
                @eventDescription
            </li>
        }
    </ol>
</section>
